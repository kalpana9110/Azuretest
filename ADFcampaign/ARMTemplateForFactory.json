			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureSqlDatabaselonkedservice')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('AzureSqlDatabaselonkedservice_connectionString')]"
				}
			},
